{
  "extends": "airbnb-base",
  "rules": {
    "linebreak-style": 0,
    "dot-notation": 0,
    "comma-dangle": 0,
    "no-param-reassign": 0,
    "func-names": 0,
    "object-shorthand": 0,
    "consistent-return": 0,
    "newline-per-chained-call": 0,
    "max-len": ["error", 120, 2, {
      ignoreUrls: true,
      ignoreComments: false,
      ignoreRegExpLiterals: true,
      ignoreStrings: true,
      ignoreTemplateLiterals: true,
    }],
    "no-new": 0,
    "no-plusplus": 0
  },
  "globals": {
    "Vue": false,
    "window": false,
    "document": false
  }
}
